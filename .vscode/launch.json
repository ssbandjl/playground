{  
    "version": "0.2.0",  
    "configurations": [  
        {  
            "name": "Debug vLLM DeepSeek-V3.2",  
            "type": "python",  
            "request": "launch",  
            "program": "${workspaceFolder}/kvcache/kv_offload_benchmark.py",  
            "console": "integratedTerminal",  
            "env": {  
                "VLLM_LOGGING_LEVEL": "ERROR",  
                "VLLM_TRACE_FUNCTION": "0",  
                "CUDA_LAUNCH_BLOCKING": "0",  
                // "VLLM_DEBUG_DUMP_PATH": "${workspaceFolder}/debug_dump", 
                // "PYTORCH_CUDA_ALLOC_CONF": "expandable_segments:True"  
            },  
            "args": [],  
            "cwd": "${workspaceFolder}",  
            "justMyCode": false,  
            "stopOnEntry": false,  
            "showReturnValue": true  
        },  
        {  
            "name": "Debug vLLM (No Function Trace)",  
            "type": "python",  
            "request": "launch",  
            "program": "${workspaceFolder}/kvcache/kv_offload_benchmark.py",  
            "console": "integratedTerminal",  
            "env": {  
                "VLLM_LOGGING_LEVEL": "DEBUG",  
                "CUDA_LAUNCH_BLOCKING": "1",  
                "VLLM_DEBUG_DUMP_PATH": "${workspaceFolder}/debug_dump",  
                "PYTORCH_CUDA_ALLOC_CONF": "expandable_segments:True"  
            },  
            "args": [],  
            "cwd": "${workspaceFolder}",  
            "justMyCode": false,  
            "stopOnEntry": false,  
            "showReturnValue": true  
        }  
    ]  
}